<IfModule mod_rewrite.c>
  RewriteEngine On RewriteBase /oasis/ RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /oasis/index.html [L]
</IfModule>
<IfModule mod_mime.c>
  AddType application/javascript .js AddType application/javascript .mjs AddType
  text/jsx .jsx AddType application/javascript .ts AddType
  application/javascript .tsx
</IfModule>
